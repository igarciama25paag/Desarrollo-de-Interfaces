<Window x:Class="FormulaFormularioa.SubWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FormulaFormularioa"
        mc:Ignorable="d"
        Title="Erantzuna" Height="200" Width="300">
    <Grid>

        <TextBlock HorizontalAlignment="Center" Margin="-200,60,0,0" TextWrapping="Wrap" Text="Emaitza" VerticalAlignment="Top"/>
        <TextBox x:Name="zenbakia" IsEnabled="False" HorizontalAlignment="Center" Margin="70,60,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>

        <Button Click="garbitu" Content="Garbitu" HorizontalAlignment="Center" Margin="-100,120,0,0" VerticalAlignment="Top" Padding="10,0"/>
        <Button Click="itxi" Content="Itxi" HorizontalAlignment="Center" Margin="100,120,0,0" VerticalAlignment="Top" Padding="20,0"/>

    </Grid>
</Window>
